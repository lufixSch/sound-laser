	journal = {NDT \& E International},
	author = {Hill, Samuel and Dixon, Steve},
	month = mar,
	year = {2014},
	keywords = {Array, Diffraction, EMAT, Frequency domain, Wave propagation},
	pages = {137--143},
	file = {ScienceDirect Snapshot:/Users/lukas/Zotero/storage/APDMI664/S0963869513001631.html:text/html},
}

